    "def listar(column):\n",
    "    biglist = column.values\n",
    "    flat_list = []\n",
    "    for sublist in biglist:\n",
    "        if type(sublist)==list:\n",
    "            for item in sublist:\n",
    "                flat_list.append(item)\n",
    "    return set(flat_list)"
   ]
